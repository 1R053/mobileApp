{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": [
      "dom",
      "esnext"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "baseUrl": "",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@components": [
        "src/components"
      ],
      "@controls/*": [
        "src/controls/*"
      ],
      "@controls": [
        "src/controls"
      ],
      "@screens/*": [
        "src/screens/*"
      ],
      "@screens": [
        "src/screens"
      ],
      "@globals/*": [
        "src/globals/*"
      ],
      "@globals": [
        "src/globals"
      ],
      "@services/*": [
        "src/services/*"
      ],
      "@services": [
        "src/services"
      ],
      "@types/*": [
        "src/types/*"
      ],
      "@types": [
        "src/types"
      ],
      "@styles/*": [
        "styles/*"
      ],
      "@styles": [
        "styles"
      ]
    }
  }
}